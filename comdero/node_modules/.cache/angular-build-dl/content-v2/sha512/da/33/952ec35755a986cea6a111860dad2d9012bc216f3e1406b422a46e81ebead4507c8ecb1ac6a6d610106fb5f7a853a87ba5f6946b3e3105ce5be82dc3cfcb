                ProductosComponent.prototype.getVideoIframe = function () {
                    var url = this.direccionVideoYoutube;
                    var video, results;
                    if (url === null) {
                        return '';
                    }
                    results = url.match('[\\?&]v=([^&#]*)');
                    video = (results === null) ? url : results[1];
                    delete this.videoPorGuardar;
                    this.videoYoutube = this._sanitizer.bypassSecurityTrustResourceUrl('https://www.youtube.com/embed/' + video);
                    delete this.videoPorGuardar;
                    this.data.video = "";
                };
                ProductosComponent.prototype.resetearVideoYoutube = function () {
