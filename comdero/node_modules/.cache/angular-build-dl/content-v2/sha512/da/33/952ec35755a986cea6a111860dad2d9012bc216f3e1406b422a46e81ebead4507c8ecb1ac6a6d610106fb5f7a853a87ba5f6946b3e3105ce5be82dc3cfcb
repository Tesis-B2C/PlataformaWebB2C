                                    if (!(eventEntrante.target.files && eventEntrante.target.files[0])) return [3 /*break*/, 6];
                                    filesAmount = eventEntrante.target.files.length;
                                    if (!(filesAmount > 6)) return [3 /*break*/, 1];
                                    this.vbanderaMensajeMaximoImagenes[indice] = true;
                                    return [3 /*break*/, 6];
                                case 1:
