                ProductosComponent.prototype.validarFormularios = function () {
                    var bandera = true;
                    for (var i in this.vectorOpciones) {
                        if (document.forms["formVariaciones" + i] != null) {
                            if (document.forms["formVariaciones" + i].checkValidity()) {
                                bandera = true;
                            }
                            else {
                                bandera = false;
                            }
                        }
                    }
                    return bandera;
                };
                ProductosComponent.prototype.validar = function () {
                    this.banderaValidaciones = true;
                    debugger;
