                MetodosEnvioComponent.prototype.transformar = function (element) {
                    debugger;
                    var valor = this.cp.transform(element.target.value, '$');
                    //let alter=formatCurrency(element.target.value,'USD',getCurrencySymbol('USD', 'wide'));
                    var valor2 = valor.split("$");
                    element.target.value = valor2[1].replace(',', "");
                };
