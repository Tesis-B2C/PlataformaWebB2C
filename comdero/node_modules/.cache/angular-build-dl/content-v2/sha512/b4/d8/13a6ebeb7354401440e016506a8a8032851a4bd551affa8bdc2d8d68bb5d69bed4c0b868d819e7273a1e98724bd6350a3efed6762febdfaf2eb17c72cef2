rores  */\\r\\n\\r\\nlabel {\\r\\n  font-size: small;\\r\\n  font-weight: normal;\\r\\n}\\r\\n\\r\\n.tamanioInputs {\\r\\n  font-size: small;\\r\\n}\\r\\n\\r\\n.alert1 {\\r\\n  font-size: smaller;\\r\\n  padding: 0;\\r\\n  background-color: white;\\r\\n  border: none;\\r\\n  color: #bd2130;\\r\\n  margin-left: 4px;\\r\\n}\\r\\n\\r\\n.alert3 {\\r\\n  font-size: smaller;\\r\\n\\r\\n\\r\\n\\r\\n}\\r\\n\\r\\n.errorSelect {\\r\\n  border: 1px solid red;\\r\\n}\\r\\n\\r\\n.satisfactorioSelect {\\r\\n  border: 1px solid limegreen;\\r\\n}\\r\\n\\r\\n.toastValidacion {\\r\\n  padding: 1%;\\r\\n  margin: 0;\\r\\n  border: red 2px solid;\\r\\n  margin-left: 75%;\\r\\n  position: absolute;\\r\\n  z-index: 2;\\r\\n  text-align: center;\\r\\n  background-color: snow;\\r\\n}\\r\\n\\r\\n.fondoAnimacionCarga {\\r\\n  background-color: rgba(0, 0, 0, 0.9);\\r\\n  width: 100%;\\r\\n  height: 100vh;\\r\\n  position: absolute;\\r\\n  z-index: 100;\\r\\n  display: flex;\\r\\n  justify-content: center;\\r\\n  align-items: center;\\r\\n}\\r\\n\\r\\n#imagenCarga {\\r\\n  width: 60%;\\r\\n  margin-left: 18px;\\r\\n}\\r\\n\\r\\n@media all and (max-width: 500px) {\\r\\n  .toastValidacion {\\r\\n    border: red 1px solid;\\r\\n    position: absolute;\\r\\n    z-index: 5;\\r\\n    text-align: center;\\r\\n    width: 100%;\\r\\n    margin: 0;\\r\\n  }\\r\\n}\\r\\n\\r\\n.body-con-animacion{\\r\\n  overflow: hidden;\\r\\n}\\r\\n\\r\\nselect{\\r\\n\\r\\n  font-size: 5pt;\\r\\n\\r\\n}\\r\\n\\r\\na:hover{\\r\\n  cursor: pointer;\\r\\n}\\r\\n\\r\\n/*# sourceMappingURL=data:application/json;base64, */\"","import {Component, OnDestroy, OnInit} from '@angular/core';\r\n\r\nimport {Agente} from \"../../modelos/agente\";\r\nimport {DpaServicio} from \"../../servicios/dpa.servicio\";\r\nimport {AgenteServicio} from \"../../servicios/agente.servicio\";\r\nimport {ToastrService} from 'ngx-toastr';\r\nimport Swal from 'sweetalert2'\r\nimport {CorreoServicio} from \"../../servicios/correo.servicio\";\r\n\r\n\r\nd