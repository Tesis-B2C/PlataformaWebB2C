(key)){const parser=stringParsers[key],match=parser.re.exec(colorString),color=match&&parser.parse(match);if(color)return color instanceof Rgba?hsva=this.rgbaToHsva(color):color instanceof Hsla&&(hsva=this.hsla2hsva(color)),hsva}return hsva}outputFormat(hsva,outputFormat,alphaChannel){switch(\"auto\"===outputFormat&&(outputFormat=hsva.a<1?\"rgba\":\"hex\"),outputFormat){case\"hsla\":const hsla=this.hsva2hsla(hsva),hslaText=new Hsla(Math.round(360*hsla.h),Math.round(100*hsla.s),Math.round(100*hsla.l),Math.round(100*hsla.a)/100);return hsva.a<1||\"always\"===alphaChannel?\"hsla(\"+hslaText.h+\",\"+hslaText.s+\"%,\"+hslaText.l+\"%,\"+hslaText.a+\")\":\"hsl(\"+hslaText.h+\",\"+hslaText.s+\"%,\"+hslaText.l+\"%)\";case\"rgba\":const rgba=this.denormalizeRGBA(this.hsvaToRgba(hsva));return hsva.a<1||\"always\"===alphaChannel?\"rgba(\"+rgba.r+\",\"+rgba.g+\",\"+rgba.b+\",\"+Math.round(100*rgba.a)/100+\")\":\"rgb(\"+rgba.r+\",\"+rgba.g+\",\"+rgba.b+\")\";default:const allowHex8=\"always\"===alphaChannel||\"forced\"===alphaChannel;return this.rgbaToHex(this.denormalizeRGBA(this.hsvaToRgba(hsva)),allowHex8)}}},ColorPickerComponent=class{constructor(elRef,cdRef,service){this.elRef=elRef,this.cdRef=cdRef,this.service=service,this.isIE10=!1,this.dialogArrowSize=10,this.dialogArrowOffset=15,this.dialogInputFields=[ColorFormats.HEX,ColorFormats.RGBA,ColorFormats.HSLA,ColorFormats.CMYK],this.useRootViewContainer=!1}handleEsc(event){this.show&&\"popup\"===this.cpDialogDisplay&&this.onCancelColor(event)}handleEnter(event){this.show&&\"popup\"===this.cpDialogDisplay&&this.onAcceptColor(event)}ngOnInit(){this.slider=new SliderPosition(0,0,0,0),this.sliderDimMax=new SliderDimension(this.hueSlider.nativeElement.offsetWidth||140,this.cpWidth,130,this.alphaSlider.nativeElement.offsetWidth||140),this.format=this.cpCmykEnabled?ColorFormats.CMYK:\"rgba\"===this.cpOutputFormat?ColorFormats.RGBA:\"hsla\"===this.cpOutputFormat?ColorFormats.HSLA:ColorFormats.HEX,this.listenerMouseDown=event=>{this.